<div class="col-7 mb-3">
  <%- if controller_name != 'sessions' %>
    <%= link_to "ログイン", new_session_path(resource_name), class: "btn btn-secondary px-4 btn-sm" %><br />
  <% end %>
</div>
<div class="col-7 mb-3">
  <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
    <%= link_to "新規登録", new_registration_path(resource_name), class: "btn btn-primary px-4 btn-sm" %><br />
  <% end %>
</div>